<h1>
  Add a new client availability
</h1>


<hr>

<form action="/create_client_availability" method="post">

  <div class="form-group">
    <label for="client_id">
      Client
    </label>

    <input type="text" id="client_id" name="client_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="slot_id">
      Slot
    </label>

    <input type="text" id="slot_id" name="slot_id" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create client availability
  </button>
</form>

<hr>

<a href="/client_availabilities">
  Go back
</a>
